{
  "manifest_version": 3,
  "name": "Small Tab",
  "version": "1.0",
  "description": "A simple and customizable new tab page.",
  

  "chrome_url_overrides": {
    "newtab": "smalltab.html"
  },
  
  "permissions": [
    "storage"
  ],
  
  "web_accessible_resources": [
    {
      "resources": ["styles/main.css", "scripts/main.js"],
      "matches": ["<all_urls>"]
    }
  ]
}